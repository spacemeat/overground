      vulkanExtensions: []
      deviceExtensions: []
      minGraphicsQueues: 1
      desiredGraphicsQueues: 2
      minComputeQueues: 1
      desiredComputeQueues: 2
      minTransferQueues: 1
      desiredTransferQueues: 2
    }
  }
}
